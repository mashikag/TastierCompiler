// This is a test program which can be compiled by the TastierCompiler.
// It reads a sequence of numbers from the input file and computes, for each
// number, the sum of all integers up to that number

program Test {
  const hope := 5;
  const hope1 := 10;
  const strTest := "Proba";
  
  Struct Rectangle{
    int width;
    int height;
  }
  
  int x [5][5];
  
  void Main() {
    int test;
    int testArray [5][5];
    
    Struct Rectangle rect;
    rect.width := 999;
    test := false ? 1 : 2;
    write test;
    
    switch(test){
      case 2{
        write 2;
        break;      
      }
      case 5{
        write 5;
        break;
      }
      default{
        write 7;      
      }
    }
    write rect.width;
    write hope;
    write strTest;
    testArray[1][1] := 5;
    write testArray[1][1];
  }
}
